{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "server/**/*",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ]
  }
  
